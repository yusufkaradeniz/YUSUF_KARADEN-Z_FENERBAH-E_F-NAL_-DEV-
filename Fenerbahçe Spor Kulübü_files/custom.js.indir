        function SendRequest(url, value) {

            $.ajax({
                url: url,
                type: 'GET',
                async: false,
                data: { p: value },
                error: function () {
                    errorStatus
                },
                success: function (msg) {
                    var data = JSON.parse(msg);
                    if (data.status == "success") {
                        if (data.data.Status != "") {
                            CRT(data.data)
                        }
                        else {
                            errorStatus()
                        }
                    } else {
                        errorStatus()
                    }
                }
            });
        };

